lock '$HOME/.local/bin/run_screenlock'
unlock '$HOME/.local/bin/run_screenunlock'

before-sleep 'loginctl lock-session'
after-resume '$HOME/.local/bin/run_screenunlock'

timeout 750 '$HOME/.local/bin/screen-dim' resume '$HOME/.local/bin/run_screenunlock'
timeout 900 'loginctl lock-session'
timeout 1200 '$HOME/.local/bin/screen-off'
# timeout 14400 'systemctl suspend-then-hibernate'
