[BottomRight]
Enter=notify-send "Disabling Screen Saver"; xset -dpms; disable-xscreensaver
Leave=notify-send "Enabling Screen Saver"; pkill -f "^/bin/sh $HOME/bin/disable-xscreensaver$"; xset +dpms
