environment {
    XDG_CURRENT_DESKTOP "niri"
    XDG_SESSION_DESKTOP "niri"
    XDG_SESSION_TYPE "wayland"
    ELECTRON_OZONE_PLATFORM_HINT "wayland"
    MOZ_DISABLE_RDD_SANDBOX "1"

    QT_QPA_PLATFORMTHEME "kde"
    QT_QPA_PLATFORM "wayland"

    SSH_ASKPASS "/home/mike/bin/yad-askpass"
    SSH_AUTH_SOCK "/run/user/1000/ssh-agent.socket"

    TERMINAL "kitty"
}

input {
    keyboard {
        xkb { }
        numlock
    }

    touchpad {
        tap
        dwt  // disable while typing
        click-method "clickfinger" // vs "button-areas"
        // natural-scroll
        // accel-speed 0.2
        // accel-profile "flat"
        scroll-method "two-finger"
        // disabled-on-external-mouse
    }

    mouse {
        // natural-scroll
        // accel-speed 0.2
        // accel-profile "flat"
        // scroll-method "no-scroll"
    }

    tablet { }
    touch { }
    trackpoint { }

    // focus-follows-mouse max-scroll-amount="95%"
    focus-follows-mouse max-scroll-amount="95%"
    warp-mouse-to-focus
    workspace-auto-back-and-forth
}

output "eDP-1" {
    position x=0 y=0
    background-color "black"
    scale 1
}

output "DP-3" {
    focus-at-startup
    position x=1920 y=0
    background-color "black"
    scale 1
}

output "DP-4" {
    position x=3840 y=0
    background-color "black"
    scale 1
}

layout {
    gaps 8
    center-focused-column "never"
    // always-center-single-column
    default-column-width { proportion 0.5; }
    preset-column-widths {
        proportion 0.33333
        proportion 0.5
        proportion 0.66667
    }

    focus-ring { off; }

    border {
        width 1
        active-color "#3daee9"
        inactive-color "#31363b"
        urgent-color "#9b0000"
    }

    shadow { }
    struts { }
}

animations { }
hotkey-overlay {
  skip-at-startup;
  // hide-not-bound;
}
debug { wait-for-frame-completion-before-queueing; }
cursor { hide-after-inactive-ms 3000; }
gestures { hot-corners { off; }; }

workspace "1"    { open-on-output "DP-3"; }
workspace "üóíÔ∏è"   { open-on-output "DP-3"; }
prefer-no-csd
screenshot-path "~/Pictures/Screenshots/Screenshot from %Y-%m-%d %H-%M-%S.png"

spawn-at-startup "systemctl" "--user" "start" "wm-session.target"
spawn-at-startup "systemctl" "--user" "start" "waybar.service"
spawn-at-startup "systemctl" "--user" "start" "swaybg.service"
spawn-at-startup "bash" "-c" "[[ ! $(hostname -s) =~ ^(styx)$ ]] && systemctl --user start swayidle.service"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(styx)$ ]] && systemctl --user start swayidle.service"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(mercury)$ ]] && systemctl --user start firefox-info.service"
spawn-at-startup "systemctl" "--user" "start" "wl-paste.service"
spawn-at-startup "systemctl" "--user" "start" "ydotool.service"
spawn-at-startup "sh" "-c" "sleep 10 && systemctl --user start syncthingtray.service"
//spawn-at-startup "gtk-launch" "ydotoold"
spawn-at-startup "gtk-launch" "blueman-applet"
spawn-at-startup "gtk-launch" "nm-applet"
spawn-at-startup "gtk-launch" "seafile-applet"
spawn-at-startup "gtk-launch" "solaar-applet"
spawn-at-startup "gtk-launch" "udiskie-applet"
spawn-at-startup "gtk-launch" "polkit-gnome-authentication-agent-1"

spawn-at-startup "sh" "-c" "$HOME/.local/bin/wm-minimize com.logseq.Logseq Logseq"
spawn-at-startup "sh" "-c" "$HOME/.local/bin/wm-minimize signal-desktop signal"
spawn-at-startup "sh" "-c" "$HOME/.local/bin/wm-minimize io.element.Element Element"
spawn-at-startup "sh" "-c" "$HOME/.local/bin/wm-minimize bitwarden"

spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(mercury)$ ]] && gtk-launch librewolf-mike"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(mercury)$ ]] && { sleep 1; gtk-launch librewolf-media; }"

spawn-at-startup "sh" "-c" "$HOME/.local/bin/run_terminal"
spawn-at-startup "bash" "-c" "sleep 1; $HOME/.local/bin/run_ssh_add"

spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(neptune|styx)$ ]] && systemctl --user start watch_tablet.service"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(neptune|styx)$ ]] && systemctl --user start screen-rotation.service"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(neptune|styx)$ ]] && systemctl --user start squeekboard.service"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(neptune|styx)$ ]] && systemctl --user start lisgd.service"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(neptune)$ ]] && niri msg output eDP-1 scale 1.2"
spawn-at-startup "bash" "-c" "[[ $(hostname -s) =~ ^(styx)$ ]] && niri msg output eDP-1 scale 1.3"

window-rule {
    match app-id=r#"^(com.seafile.seafile-applet|galculator|imv|nm-connection-editor|nwg-drawer|nwg-look|onboard|org.gnome.Nautilus|org.kde.plasma.emojier|piper|thunar|solaar|syncthingtray|vimiv)$"#
    match app-id=r#"^(librewolf|firefox)$"# title=r#"^($|(Firefox|LibreWolf) - Choose User Profile|Enter name of file to save to‚Ä¶|Library$|Page Info)"#
    match app-id="org.remmina.Remmina" title="Remmina Remote Desktop Client"
    match title=r#"^(Open File|Open|Save|Save As|Export|Import|Choose File|Rename)"#
    match title=r#"^[Xx]dg-desktop-portal-(gnome|gtk|hyprland|wlr)"#
    open-floating true
    min-width 640
    min-height 480
}

window-rule {
    match app-id=r#"^(firefox|fr.handbrake.ghb|gimp|labwc|librewolf|obsidian|vlc)$"#
    open-maximized true
}

window-rule {
    match app-id=r#"^(firefox|librewolf)$"# at-startup=true
    open-on-output "DP-4"
}

window-rule {
    match app-id=r#"^org\.wezfurlong\.wezterm$"#
    default-column-width {}
}

window-rule {
    match title="Enter SSH Key Passphrase" at-startup=true
    open-floating true
    open-focused true
}

window-rule {
    match app-id=r#"(firefox|librewolf)$"# title="^Picture-in-Picture$"
    open-floating true
}

window-rule {
    match app-id=r#"^(Bitwarden|blueman-manager|Element|io.missioncenter.MissionCenter|Logseq|signal|pavucontrol|htop|btop|net.nokyan.Resources|nvtop|term-scratchpad|termfm)$"#
    open-floating true
    default-column-width { proportion 0.95; }
    default-window-height { proportion 0.90; }
    open-maximized false
}

window-rule {
    match app-id=r#"^(Bitwarden|blueman-manager|Element|io.missioncenter.MissionCenter|Logseq|signal|pavucontrol|htop|btop|net.nokyan.Resources|nvtop|term-scratchpad|termfm)$"# at-startup=true
    open-on-workspace "üóíÔ∏è"
    open-floating false
    open-focused false
}

