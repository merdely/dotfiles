    Mod+E                    { spawn "true"; }
    Mod+I                    { spawn "true"; }
    Mod+P                    { spawn "true"; }
    Mod+S                    { spawn "true"; }
    Mod+U                    { spawn "true"; }
    Mod+W                    { spawn "true"; }
    Mod+Z                    { spawn "true"; }
