{
    "custom/toggle_winchooser": {
        "format": "",
        "tooltip": true,
        "tooltip-format": "Window Switcher",
        "on-click": "$HOME/.local/bin/wm-window-switcher"
    },
    "custom/toggle_scratchchooser": {
        "format": "",
        "tooltip": true,
        "tooltip-format": "Scratchpad Chooser",
        "on-click": "$HOME/.local/bin/run-app-toggle -S \"([r]ofi|[f]uzzel) -p Scratchpad App > ? -d(menu)?\" wm-scratchpad-chooser"
    }
}
